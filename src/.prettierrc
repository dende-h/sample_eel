{
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": true,
    "semi": true,
    "singleQuote": false,
    "overrides": [{ "files": "*.js", "options": { "singleQuote": true } }],
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "trailingComma": "none",
    "embeddedLanguageFormatting": "off"
}
